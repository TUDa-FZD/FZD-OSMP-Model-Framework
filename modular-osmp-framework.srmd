        <ClassificationEntry keyword="tool.intended-use.qualification.status"></ClassificationEntry>
        <ClassificationEntry keyword="tool.intended-use.qualification.report"></ClassificationEntry>

        <ClassificationEntry keyword="tool.limitations"></ClassificationEntry>
    
        <ClassificationEntry keyword="tool.supported.formats">OSI, OSMP, FMI</ClassificationEntry>
        <ClassificationEntry keyword="tool.installation.requirements">https://gitlab.com/tuda-fzd/perception-sensor-modeling/modular-osmp-framework/-/blob/master/README.md</ClassificationEntry>
        
    </Classification>
	
